"VertexLitGeneric"
{

	  $basetexture "models\weapons\v_colognebottle\cologne_glass_blu_texture"
                  $basemapalphaphongmask "1"

                "$phongwarptexture" "models\weapons\v_colognebottle/cologne_bluglass_lightwarp"
                "$lightwarptexture" "models\weapons\v_colognebottle/cologne_bluglass_lightwarp"

     
                "$phong" "1"
	"$phongexponent" "40"
	"$phongfresnelranges" "[1 2 1]"
	"$phongboost" "2"

	"$rimlight" "1"						
	"$rimlightexponent" "25"				
	"$rimlightboost" "0.2"





	">=DX90"
	{
	"$selfIllum" "1"
	"$envmap" "env_cubemap"
	"$envmaptint" "[.1 .2 .1]"
	}
//	"$baseAlphaEnvMapMask" "1" 



	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}