"VertexLitGeneric"
{
	"$baseTexture" "models/weapons/c_models/c_no_feelings/c_NoFeelingsSight_Diffuse"
	"$bumpmap" "models/weapons/c_models/c_no_feelings/c_NoFeelingsSight_normal"
	
	$nocull 1
	$phongexponenttexture "models/weapons/c_models/c_no_feelings/c_nofeelingsbody_e"


	$envmap "env_cubemap"
	$normalmapalphaenvmapmask 1
	$envmaptint "[0.5 0 0]"
	$envmapfresnel 1

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}