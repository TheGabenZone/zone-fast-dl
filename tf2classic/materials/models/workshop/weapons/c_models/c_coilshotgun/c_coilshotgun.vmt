"VertexLitGeneric"
{
	"$basetexture" "models\workshop\weapons\c_models\c_coilshotgun\c_coilshotgun"

	"$BASEMAPALPHAPHONGMASK" "1"

	"$envmap" "env_cubemap"
	"$envmaptint" "[.2 .2 .2]"

	"$phong" "1"
	"$phongexponent" "20"
	"$phongboost" "4"

	"$phongfresnelranges" "[.25 .5 1]"

	"$lightwarptexture" "models\lightwarps\softened_weapon_lightwarp"

	"$rimlight" "1"
	"$rimlightexponent" "5"	
	"$rimlightboost" ".5"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
		{
		"AnimatedTexture"
		{
		"animatedTextureVar" "$basetexture"
		"animatedTextureFrameNumVar" "$frame"
		"animatedTextureFrameRate" "3"
		}
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}
